{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.abc",
        "headers": [
          {"key": "Content-Type", "value": "text/vnd.abc"}
        ]
      },
      {
        "source": "**/*.mscx",
        "headers": [
          {"key": "Content-Type", "value": "application/vnd.musescore+xml"}
        ]
      },
      {
        "source": "**/*.mscz",
        "headers": [
          {"key": "Content-Type", "value": "application/vnd.musescore+zip"}
        ]
      }
    ]
  }
}
